<h1>Percentage of fields meeting 'T'</h1>

<table>
	<thead>
		<tr>
			<th>Year</th>
			<th>Fields at or below T</th>
			<th>Total Fields</th>
			<th>Percentage of fields meeting 'T'</th>
		</tr>
	</thead>
	<tbody>
<% @calcYear.each do |r| %>
  <%  @tCount = Season.active.byYear(r).tol.count %>
  <%  @yCount = Season.active.byYear(r).count.to_f %>
		<tr>
			<td><%= r %></td>
			<td><%= @tCount %></td>
			<td><%= @yCount %></td>
			<td><%= '%.2f'%((@tCount/@yCount)*100) + '%' %></td>
		</tr>
<% end %>
	</tbody>
</table>
<br />
<br />
